<!DOCTYPE html>
<!--[if IE 8]> <html lang="en" class="ie8"> <![endif]-->
<!--[if IE 9]> <html lang="en" class="ie9"> <![endif]-->
<!--[if !IE]><!-->
<html lang="en" ng-app="app">
<!--<![endif]-->
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - @AppSettings.String("ApplicationName")</title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta content="width=device-width, initial-scale=1" name="viewport" />
    <meta content="" name="description" />
    <meta content="" name="author" />
    <link href="http://fonts.googleapis.com/css?family=Open+Sans:400,300,600,700&subset=all" rel="stylesheet" type="text/css" />
    @Styles.Render("~/stylebundles/metronic-global")
    @Styles.Render("~/stylebundles/metronic-theme")
    @Styles.Render("~/stylebundles/site")
    @RenderSection("styles", required: false)
    <link rel="icon" href="@Url.Content("~/wwwroot/favicon.ico")" />
</head>
<body class="page-header-fixed page-sidebar-closed-hide-logo page-content-white">
    <div class="page-wrapper">
        @Html.Partial("_Header")
        <div class="clearfix"></div>
        <div class="page-container">
            @Html.Partial("_Sidebar")
            @RenderBody()
            @Html.Partial("_QuickSidebar")
        </div>
        @Html.Partial("_Footer")
    </div>
    @Scripts.Render("~/scriptbundles/metronic-core")
    @Scripts.Render("~/scriptbundles/metronic-theme-global")
    @Scripts.Render("~/scriptbundles/metronic-theme-layout")
    @Scripts.Render("~/scriptbundles/angular")
    <script>
        (function () {
            window.global = {
                
            };
        }());
    </script>
    <script src="~/wwwroot/scripts/shared/app.js"></script>
    @RenderSection("scripts", required: false)
</body>
</html>